    "deploy": "npm run build:server && npm run docker:build && npm run docker:push",
    "docker:build": "docker build -t dhub.nvx.me:443/ngx-kit-ui-alma .",
    "docker:run": "docker run -d -p 4001:4000 dhub.nvx.me:443/ngx-kit-ui-alma",
    "docker:push": "docker push dhub.nvx.me:443/ngx-kit-ui-alma"
  },
  "private": true,
